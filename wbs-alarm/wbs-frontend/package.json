    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "^11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.6",
    "@angular/router": "^11.2.6",
    "@plentymarkets/terra-components": "^11.0.0",
    "@plentymarkets/terra-icons": "latest",
    "angular-l10n": "^11.0.0",
    "bootstrap": "^4.4.1",
    "ckeditor4-angular": "^1.0.1",
    "htmlhint": "~0.10.3",
    "iban": "^0.0.14",
    "interactjs": "1.3.4",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.24.0",
    "mydatepicker": "^9.0.2",
    "ng2-ace-editor": "^0.3.9",
    "ngx-bootstrap": "^6.1.0",
    "ngx-quill": "^13.2.0",
    "path": "^0.12.7",
    "quill": "^1.3.7",
    "rxjs": "^6.5.4",
    "tippy.js": "^5.1.4",
