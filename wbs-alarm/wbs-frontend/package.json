  "scripts": {
    "start": "ng serve --aot",
    "just_build": "ng build --prod",
    "build": "ng build --prod",
    "clean_all_win": "rd /s /q \"node_modules/\" && rd /s /q \"node/\" && rd /s /q \"dist/\"",
    "clean_all_ux": "rm -rf node_modules/ && rm -rf node/ && rm -rf dist/",
    "clean:dist": "npm run rimraf -- dist",
    "rimraf": "rimraf",
    "test": "ng test",
    "test-headless": "ng test --browsers=ChromeHeadless",
    "test-headless-coverage": "ng test --watch=false --codeCoverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "reinstall_win": "rd /s /q \"node_modules\" && npm i",
    "reinstall_ux": "rm -rf node_modules/ && npm i"
  },
