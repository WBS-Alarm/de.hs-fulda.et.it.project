{
  "name": "wbs",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "ng serve --aot",
    "just_build": "ng build --prod",
    "build": "ng build --prod",
    "clean_all_win": "rd /s /q \"node_modules/\" && rd /s /q \"node/\" && rd /s /q \"dist/\" && rd /s /q \"coverage/\"",
    "clean_all_ux": "rm -rf node_modules/ && rm -rf node/ && rm -rf dist/ && rm -rf coverage/",
    "clean:dist": "npm run rimraf -- dist",
    "rimraf": "rimraf",
    "test": "ng test",
    "test-headless": "ng test --browsers=ChromeHeadless",
    "test-headless-coverage": "ng test --watch=false --codeCoverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "reinstall_win": "if exist \"node_modules\" (rd /s /q \"node_modules\" && npm i) && (npm i)",
    "reinstall_ux": "rm -rf node_modules/ && npm i"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.6",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "^11.2.6",
