	"scripts": {
		"start": "webpack-dev-server --progress --watch --profile --config config/webpack.dev.js",
		"just_build": "webpack --progress --profile --bail --display-error-details --config config/webpack.prod.js",
		"build": "npm run clean:dist && webpack --progress --profile --bail --display-error-details --config config/webpack.prod.js",
		"clean_all": "rm -rf node_modules/ && rm -rf node/ && rm -rf dist/",
		"clean:dist": "npm run rimraf -- dist",
		"rimraf": "rimraf",
		"test": "npm run lint",
		"lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
		"reinstall": "rm -rf node_modules/ && npm i"
	},
